
@using WebApplication1.Models;
@{
    var session = Session["SessionUser"] as SessionModels;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Grupo Enel - Programa de protección contra caídas</title>
    @Styles.Render("~/Content/SemanticUi/semantic.css")
    @Styles.Render("~/Content/Site.css")
    @Styles.Render("~/Content/font-awesome-4.7.0/css/font-awesome.css")
    <link href="~/Content/facade.css" rel="stylesheet" />

    <link href="~/Content/SemanticUI/semantic.css" rel="stylesheet" />
    <link href="~/Scripts/tables/jquery.dynatable.css" rel="stylesheet" />
    <link href="~/Content/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Scripts/calendar/calendar.min.css" rel="stylesheet" />
    <link href="~/Content/toastr.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jqueryJs")
    @Scripts.Render("~/bundles/semanticJs")
    @Scripts.Render("~/bundles/loadingOverlayJs")
    @Scripts.Render("~/bundles/jqueryUnobstriveJs")
    <script type="text/javascript" src="~/Scripts/tables/jquery.dynatable.js"></script>
    <script type="text/javascript" src="~/Scripts/calendar/calendar.min.js"></script>
    <script src="~/Scripts/toastr.js"></script>
    <script src="~/Scripts/dropzone/dropzone.min.js"></script>
    <link href="~/Scripts/dropzone/dropzone.min.css" rel="stylesheet" />
    <script src="~/Scripts/chartJs/Chart.bundle.min.js"></script>
    <script src="~/Scripts/chartJs/Chart.min.js"></script>
    <link href="~/Content/menuNew.css" rel="stylesheet" />


    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>

    <style>
        body {
            background-image: url('@Url.Content("~/Content/Images/02-Pantalla-ingreso-A.jpg")');
            background-repeat: no-repeat;
            background-size: cover;
        }
        .card-header .fa {
            transition: .3s transform ease-in-out;
        }

        .card-header .collapsed .fa {
            transform: rotate(90deg);
        }

        .card-bdy {
            background: linear-gradient(to bottom, #ddefe0,#54c4a4);
        }
        .btn-bdy {
            color: #00B5AD !important;
            font-size: 16px;
            font-weight:bold;
        }
    </style>
</head>
<body>
    <div class="main">
        <div class="ui equal width form" style="margin-top: 2%;">
            <div class="fields">
                <div class="field" style="width: 27% !important;">
                    <div class="accordion" id="accordionExample">
                        <div class="card">
                            <div class="card-header" id="headingOne">
                                <h2 class="mb-0 tag tag-blue">
                                    <button class="btn btn-link btn-block text-left btn-bdy" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                        Programa alturas
                                    </button>
                                </h2>
                            </div>

                            <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
                                <div class="card-body card-bdy">
                                    <ul class="sub-menu">
                                        <li class="menu-item"><a class="menu__link" href="#" onclick="loadDummyData('@Url.Action("ProceduresReportsIndex", "Inspections")')">Procedimientos</a></li>
                                        <li class="menu-item"><a class="menu__link" href="#" onclick="loadDummyData('@Url.Action("FormatsReportsIndex", "Inspections")')">Formatos</a></li>
                                        <li class="menu-item"><a class="menu__link" href="#" onclick="loadDummyData('@Url.Action("CertificatesReportsIndex", "Inspections")')">Normatividad</a></li>
                                        <li class="menu-item"><a class="menu__link" href="#" onclick="loadDummyData('@Url.Action("Features", "Inspections")')">Cargar Documentos</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="card">
                            <div class="card-header" id="headingTwo">
                                <h2 class="mb-0 tag tag-blue">
                                    <button class="btn btn-link btn-block text-left collapsed  btn-bdy" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                        Gestión de Equipos
                                    </button>
                                </h2>
                            </div>
                            <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionExample">
                                <div class="card-body card-bdy">
                                    <ul class="sub-menu">
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("Stock", "WorkingAtHeight")')">Inventario</a></li>
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("TechnicalInformation", "WorkingAtHeight")')">Fichas técnicas</a></li>
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("CreateTechInfo", "Inspections")')">Cargar Fichas técnicas</a></li>
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("Curriculum", "WorkingAtHeight")')">Hojas de Vida</a></li>
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("InspectionCert", "WorkingAtHeight")')">Certificados de Inspección</a></li>
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("IndexEquipment", "WorkingAtHeight")')">Página Inicial de Equipos</a></li>
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("CreateEquipment", "WorkingAtHeight")')">Crear Nuevo Equipo</a></li>
                                        <li class="menu-item"><a href="#" class="menu__link" onclick="loadDummyData('@Url.Action("DownloadResult", "WorkingAtHeight")')">Matriz de Equipos</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="card">
                            <div class="card-header" id="headingThree">
                                <h2 class="mb-0 tag tag-blue">
                                    <button class="btn btn-link btn-block text-left collapsed btn-bdy" type="button" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                        Inspección y Mantenimiento
                                    </button>
                                </h2>
                            </div>
                            <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordionExample">
                                <div class="card-body card-bdy">
                                    <ul data-menu="submenu-3" class="menu__level">
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("SchedulerInspections", "Inspections")">Cronograma Inspecciones</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateInspections", "Inspections")">Registros Inspecciones</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("TechnicalReportsIndex", "Inspections")">Informes técnicos (LV)-Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateTechnicalReports", "Inspections")">Informes técnicos (LV)-Crear Nuevo</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("ManagementOfFindings", "Inspections")">Gestión de Hallazgos</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("ManagementMaintenanceEquipment", "Inspections")">Verificar Gestión</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>

                        <div class="card">
                            <div class="card-header" id="heading4">
                                <h2 class="mb-0 tag tag-blue">
                                    <button class="btn btn-link btn-block text-left collapsed btn-bdy" type="button" data-toggle="collapse" data-target="#collapse4" aria-expanded="false" aria-controls="collapse4">
                                        Formación y Competencia
                                    </button>
                                </h2>
                            </div>
                            <div id="collapse4" class="collapse" aria-labelledby="heading4" data-parent="#accordionExample">
                                <div class="card-body card-bdy">

                                    <ul data-menu="submenu-4" class="menu__level">
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("Trainning", "Trainning")">Listado/Rol/Aptitud</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("Scheduler", "Trainning")">Cronograma Formación</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("Certificate", "Trainning")">Certificados</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("IndexPerson", "Trainning")">Personas-Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreatePerson", "Trainning")">Personas-Crear Nuevo</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>


                        <div class="card">
                            <div class="card-header" id="heading5">
                                <h2 class="mb-0 tag tag-blue">
                                    <button class="btn btn-link btn-block text-left collapsed btn-bdy" type="button" data-toggle="collapse" data-target="#collapse5" aria-expanded="false" aria-controls="collapse5">
                                        Administración
                                    </button>
                                </h2>
                            </div>
                            <div id="collapse5" class="collapse" aria-labelledby="heading5" data-parent="#accordionExample">
                                <div class="card-body card-bdy">
                                    <ul data-menu="submenu-5-1" class="menu__level">
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("IndexUser", "Trainning")">Usuarios - Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateUser", "Trainning")">Usuarios - Crear Nuevo</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("IndexRol", "Trainning")">Roles - Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateRol", "Trainning")">Roles - Crear Nuevo</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("IndexInspector", "Trainning")">Inspectores - Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateInspector", "Trainning")">Inspectores - Crear Nuevo</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("IndexSedeCategoria", "WorkingAtHeight")">Categoria Sedes - Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateCategoriaSede", "WorkingAtHeight")">Categoria Sedes - Crear Nuevo</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("IndexPais", "WorkingAtHeight")">Países - Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreatePais", "WorkingAtHeight")">Países - Crear Nuevo</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("IndexSede", "WorkingAtHeight")">Sedes - Página Inicial</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateSede", "WorkingAtHeight")">Sedes - Crear Nuevo</a></li>

                                    </ul>
                                </div>
                            </div>
                        </div>


                        <div class="card">
                            <div class="card-header" id="heading6">
                                <h2 class="mb-0 tag tag-blue">
                                    <button class="btn btn-link btn-block text-left collapsed btn-bdy" type="button" data-toggle="collapse" data-target="#collapse6" aria-expanded="false" aria-controls="collapse6">
                                        Indicadores de Gestión
                                    </button>
                                </h2>
                            </div>
                            <div id="collapse6" class="collapse" aria-labelledby="heading6" data-parent="#accordionExample">
                                <div class="card-body card-bdy">
                                    <ul data-menu="submenu-5-1" class="menu__level">
                                        <li class="menu__item">
                                            <a class="menu__link" href="#" onclick="loadDummyData('@Url.Action("PageInitial", "ManagmentIndicators")')">Indicadores</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>



                        <div class="card">
                            <div class="card-header" id="heading7">
                                <h2 class="mb-0 tag tag-blue">
                                    <button class="btn btn-link btn-block text-left collapsed btn-bdy" type="button" data-toggle="collapse" data-target="#collapse7" aria-expanded="false" aria-controls="collapse7">
                                        Módulo Médico
                                    </button>
                                </h2>
                            </div>
                            <div id="collapse7" class="collapse" aria-labelledby="heading7" data-parent="#accordionExample">
                                <div class="card-body card-bdy">
                                    <ul data-menu="submenu-7" class="menu__level">
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("CreateAptitudConcept", "MedicalConcepts")">Conceptos de Aptitud</a></li>
                                        <li class="menu__item"><a class="menu__link" href="@Url.Action("PageInitial", "MedicalConcepts")">Listado de Aptitud</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>




                    </div>
                </div>

                <div class="field">
                    <div class="ui segment" id="renderPages" style="min-height:700px !important">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/jqueryJs")
    @Scripts.Render("~/bundles/semanticJs")
    @Scripts.Render("~/Content/LoadingOverlay/src/loadingoverlay.min.js")
    @Scripts.Render("~/bundles/jqueryUnobstriveJs")
    @RenderSection("scripts", required: false)
    <script type="text/javascript">

        
    function loadDummyData(hrefURL) {
            cargarElement(hrefURL);
        };
    function cargarElement(url) {
        $.ajax({
            url: url,
            beforeSend: function () {
                $('#renderPages').html('').addClass("loading");
                /*$('#renderPages').html('').addClass("loading").css("height", 500);*/
            },
            success: function (data) {
                $('#renderPages').html(data);
                $('.titlesPages').html('@ViewBag.ApplicationName | ' + $('.titlesPages').html());
            },
            complete: function () {
                $('#renderPages').removeClass("loading").css("height", "inherit");
                LoadElementsDefaultPartial('#renderPages');
        /*        $("#menuPartial").hide();*/
            }
        })
    }
    function LoadElementsDefaultPartial(element) {
        $(element).find($('.ui.dropdown').dropdown());
        $(element).find($('.ui.checkbox').checkbox());
        }
 

    toastr.options = {
    "closeButton": false,
    "debug": false,
    "newestOnTop": false,
    "progressBar": true,
    "positionClass": "toast-bottom-right",
    "preventDuplicates": false,
    "onclick": null,
    "showDuration": "300",
    "hideDuration": "0",
    "timeOut": "5000",
    "extendedTimeOut": "1000",
    "showEasing": "swing",
    "hideEasing": "linear",
    "showMethod": "fadeIn",
    "hideMethod": "fadeOut"
    }
    $.fn.noop = function () { return this; };
    toastr.options.hideMethod = 'noop';
    @*$.ajax({
            url: '@Url.Action("GetMissions", "Mission")',
            type: 'POST',
            success: function (data) {
                toastr.info("Próximas Misiones");
                if (data.result) {
                    $.each(data.mission, function (index, value) {
                        toastr.success("Fecha: " + value.date + " - " + value.city + '<br/> ' + value.description, value.name);
                    });
                }

            }
        });*@
    function LoadElementsDefault() {
    $('.ui.dropdown').dropdown();
    }
    $(function () {
    LoadElementsDefault();
    });
    function LoadTables() {
    $(function () {
    $('.tablesInitial').dynatable();
    });
    }
    $('#renderPages').on('click', function () {
    $("#menuPartial").hide();
    });
    </script>
    @RenderSection("scripts", required: false)

</body>
</html>
